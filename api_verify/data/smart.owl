        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#severity"/>
                <owl:allValuesFrom rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
            </owl:Restriction>
        </owl:annotatedTarget>
    </owl:Axiom>
    <owl:Axiom>
        <rdfs:comment>Category of an allergy (food, drug, other substance).</rdfs:comment>
        <owl:annotatedSource rdf:resource="http://smartplatforms.org/terms#Allergy"/>
        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#category"/>
